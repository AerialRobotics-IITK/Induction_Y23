<?xml version="1.0" encoding="utf-8"?>

<launch>
	<include file="$(find smb_gazebo)/launch/smb_gazebo.launch">
		<arg name="world_file" value="worlds/robocup14_spl_field.world"/>
		<arg name="laser_enabled" value="true"/>	
	</include>

<node pkg="smb_highlevel_controller" type="smb_highlevel_controller" name="smb_highlevel_controller">
	<rosparam command="load" file="$(find smb_highlevel_controller)/config/config.yaml"/>
</node>
<node type="rviz" name="rviz" pkg="rviz" args="-d $(find smb_highlevel_controller/config/smb_gazebo.rviz" />
</launch>






